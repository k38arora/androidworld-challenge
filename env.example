# AndroidWorld Challenge Environment Configuration
# Copy this file to .env and customize the values

# Genymotion Cloud Credentials (Required for challenge)
GENYMOTION_USERNAME=your-genymotion-username
GENYMOTION_PASSWORD=your-genymotion-password
GENYMOTION_LICENSE_KEY=your-license-key

# Genymotion Cloud Configuration
GENYMOTION_CLOUD_API_URL=https://cloud.geny.io/api/v1
GENYMOTION_DEVICE_TEMPLATE=Google Pixel 4
GENYMOTION_ANDROID_VERSION=11.0

# Google Cloud Configuration (Optional - uses gcloud config if not set)
GOOGLE_CLOUD_PROJECT=your-project-id-here
GOOGLE_CLOUD_REGION=us-central1
GOOGLE_CLOUD_ZONE=us-central1-a

# Instance Configuration
INSTANCE_NAME=genymotion-emulator
MACHINE_TYPE=n1-standard-4
DISK_SIZE=50GB

# Agent System Configuration
DEFAULT_EPISODES=50
DEFAULT_AGENT_TYPE=orchestrator
DEFAULT_OUTPUT_FORMAT=json

# Emulator Configuration
EMULATOR_IP=localhost
EMULATOR_PORT=5555

# Task Type Configuration
ENABLE_NAVIGATION_TASKS=true
ENABLE_INSTALLATION_TASKS=true
ENABLE_INFORMATION_GATHERING_TASKS=true
ENABLE_CAPTURE_TASKS=true
ENABLE_MAINTENANCE_TASKS=true

# Docker Configuration
DOCKER_IMAGE_NAME=candidate/android-world
DOCKER_CONTAINER_NAME=androidworld-challenge
